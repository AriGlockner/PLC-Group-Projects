function fact(n) \{\
  var f = 1;\
  while (n > 1) \{\
    f = f * n;\
    n = n - 1;\
  \}\
  return f;\
\}\
\
function binom(a, b) \{\
  var val = fact(a) / (fact(b) * fact(a-b));\
  return val;\
\}\
